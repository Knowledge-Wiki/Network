{
	"name": "Network",

	"version": "0.1.0",

	"author": [
		"[https://professional.wiki/ Professional.Wiki]",
		"[https://www.entropywins.wtf/mediawiki Jeroen De Dauw]"
	],

	"url": "https://github.com/ProfessionalWiki/Network#network-extension",

	"descriptionmsg": "network-desc",

	"license-name": "GPL-2.0-or-later",

	"type": "parserhook",

	"requires": {
		"MediaWiki": ">= 1.31.0"
	},

	"MessagesDirs": {
		"Network": [
			"i18n"
		]
	},

	"ExtensionMessagesFiles": {
		"NetworkParserFunction": "i18n/_MagicWords.php"
	},

	"AutoloadClasses": {
		"MediaWiki\\Network\\NetworkExtension": "NetworkExtension.php"
	},

	"Hooks": {
		"ParserFirstCallInit": "MediaWiki\\Network\\NetworkExtension::onParserFirstCallInit"
	},

	"ResourceFileModulePaths": {
		"localBasePath": "resources",
		"remoteExtPath": "Network/resources"
	},

	"ResourceModules": {
		"ext.network": {
			"scripts": [
				"lib/d3.js",
				"js/Page.js",
				"js/ApiPageConnectionRepo.js",
				"js/GraphElements.js",
				"js/InteractiveGraph.js",
				"js/Network.js",
				"js/index.js"
			],
			"targets": [ "desktop", "mobile" ]
		}
	},

	"manifest_version": 2
}
